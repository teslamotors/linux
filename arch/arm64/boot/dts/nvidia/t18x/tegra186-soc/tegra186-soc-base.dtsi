#include "tegra186-soc/tegra186-soc-uart.dtsi"
#include "tegra186-soc/tegra186-soc-eqos.dtsi"
#include "tegra186-soc/tegra186-soc-power-domain.dtsi"
#include "tegra186-soc/tegra186-trusty.dtsi"
