		       0x0 0x40000000 0x0 0x10000000   /* configuration space */
		       0x0 0x02520000 0x0 0x00060000>; /* UPHY Lane registers */
		reg-names = "pads", "afi", "cs", "lanes";

		clocks = <&tegra_car TEGRA186_CLK_AFI>,
			<&tegra_car TEGRA186_CLK_PCIE>,
