	host1x {
		i2c@546c0000 {
			tca9548@6_0077 {
				compatible = "nxp,pca9548";
				reg = <0x77>;
				#address-cells = <1>;
				#size-cells = <0>;
				vcc-supply = <&en_vdd_cam>;
